<ul>
  <% books.forEach((book, index) => { %>
    <li>
      <img src= <%= book.image %> >
      <h2><%= book.title %></h2>
      <h3><%= book.authors %></h3>
      <p><%= book.description %></p>
      <p><%= book.ID %></p>
      <p>Bookshelf: Fantasy</p>
      <p><%= index + 1 %> Of <%= books.length %></p>
    </li>
  <% }); %>
</ul>